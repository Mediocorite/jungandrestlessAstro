{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
        "strictNullChecks": true,
            "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
        "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@y/*": ["src/layouts/*"],
      "@/*": [""],
      "@config/*": ["config/*"],
      "@public/*": ["public/*"]
    }
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.astro"
  ]
}